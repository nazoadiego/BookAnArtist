<%= simple_form_for [@offer, @booking] do |f| %>
  <%= f.input :start_time, as: :string, label: 'Start Date', input_html: {class: 'datepicker', id: 'start date'} %>
  <%= f.input :end_time, as: :string, label: 'End Date', input_html: {class: 'datepicker', id: 'end date'} %>
  <%= f.submit 'Book Now!'%>
<% end %>
